{
  "name": "codex-team-orchestrator",
  "version": "1.0.0",
  "description": "Production-ready multi-agent team orchestrator for Codex",
  "type": "module",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "format": "node --import tsx scripts/format.ts",
    "lint": "node --import tsx scripts/lint.ts",
    "test": "node --import tsx --test tests/**/*.test.ts",
    "test:unit": "node --import tsx --test tests/unit/**/*.test.ts",
    "test:integration": "node --import tsx --test tests/integration/**/*.test.ts",
    "test:ts": "node --import tsx --test tests/**/*.test.ts",
    "test:unit:ts": "node --import tsx --test tests/unit/**/*.test.ts",
    "test:integration:ts": "node --import tsx --test tests/integration/**/*.test.ts",
    "typecheck": "tsc -p tsconfig.json",
    "build:types": "tsc -p tsconfig.json --noEmit false --emitDeclarationOnly --declaration --declarationMap false --outDir dist/types",
    "verify": "./scripts/verify.sh",
    "benchmark": "./scripts/benchmark.sh --baseline fixed-6 --candidate adaptive",
    "release:package": "./scripts/package-release.sh",
    "release:check": "./scripts/release-ready.sh"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
