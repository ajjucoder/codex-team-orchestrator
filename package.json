{
  "name": "codex-team-orchestrator",
  "version": "1.0.0",
  "description": "Production-ready multi-agent team orchestrator for Codex",
  "type": "module",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "format": "node scripts/format.mjs",
    "lint": "node scripts/lint.mjs",
    "test": "node --test tests/**/*.test.js",
